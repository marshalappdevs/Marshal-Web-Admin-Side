div(dir='rtl')#courseViewPanel.panel.panel-primary
    .panelHeading.panel-heading {{ toDisplay.name }}
    table.table.modalTable(dir='rtl')
        tr.row(ng-repeat="prop in columns", ng-if="prop.realName !== 'name'")
            td.coursesTableHead.col-md-3 {{ prop.hebName }}
            td.col-md-9(ng-bind-html="trustAsHtml(toDisplay[prop.realName].toString())")